//#SUB
//#############################################################################
//#
//# 【共通】店舗マスタ処理
//#
//#############################################################################

$echo ■店舗マスタ読込処理開始

//#############################################################################
//#	パラメータセット
//#############################################################################
//# %1=Commonファイルパス
$ParaSet("%COMMON_PATH",%1)

//# %2=マクロファイルパス
$ParaSet("%MAC_PATH",%2)

//# %3=D5T格納パス
$ParaSet("%D5T_PATH",%3)

//# %4=CSVパス
$ParaSet("%CSV_PATH",%4)

//# %5=カタログパス
$ParaSet("%CTLG_PATH",%5)

//# %6=法人コード
$ParaSet("%COMP_CD",%6)

//# ---------------------------------------------------------------------------
//# 店舗マスタ処理
//# ---------------------------------------------------------------------------

//# 店舗マスタの読込
$Load("%D5T_PATH","R_TENPO.D5T")
$Rename("R_TENPO","R_TENPO_D5T")
$SetComment("R_TENPO","1","READ_R_TENPO")


//# 法人コードで絞りこむ
$MultiSearch("R_TENPO","1","""HOJIN_CD"" = '%COMP_CD'")

$SetComment("R_TENPO","2","WHERE HOJIN_CD")


$echo ■店舗マスタ読込処理終了
