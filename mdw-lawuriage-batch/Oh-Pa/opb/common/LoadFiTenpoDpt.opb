//#############################################################################
$echo ■会計用店DPTマスタ処理開始
//#############################################################################


//#############################################################################
//#	パラメータセット
//#############################################################################
//# %1=Commonファイルパス
$ParaSet("%COMMON_PATH",%1)

//# %2=マクロファイルパス
$ParaSet("%MAC_PATH",%2)

//# %3=D5T格納パス
$ParaSet("%D5T_PATH",%3)

//# %4=CSVパス
$ParaSet("%CSV_PATH",%4)

//# %5=カタログパス
$ParaSet("%CTLG_PATH",%5)

//# %6=法人コード
$ParaSet("%COMP_CD",%6)

//# ---------------------------------------------------------------------------
//# 会計用店DPTマスタの読込
//# ---------------------------------------------------------------------------

$Load("%D5T_PATH","R_FI_TENPO_BUNRUI1.D5T")
$Rename("R_FI_TENPO_BUNRUI1","R_FI_TENPO_BUNRUI1_D5T")
$SetComment("R_FI_TENPO_BUNRUI1","1","READ_R_FI_TENPO_BUNRUI1")

//# ---------------------------------------------------------------------------
//# 法人コードで絞込
//# ---------------------------------------------------------------------------
$MultiSearch("R_FI_TENPO_BUNRUI1","1","""COMP_CD"" = '%COMP_CD'")
$SetComment("R_FI_TENPO_BUNRUI1","2","WHERE COMP_CD")


//#############################################################################
$echo ■会計用店DPTマスタ読込終了
//#############################################################################
